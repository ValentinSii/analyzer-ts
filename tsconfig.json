{
    "extends": "@apify/tsconfig",
    "compilerOptions": {
        "module": "ES2022",
        "target": "ES2022",
        "skipLibCheck": true,
        "outDir": "dist",
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitAny": false,
        "paths": { "@frontend/*": ["../analyzer-ui"]},
    },
    "include": [
        "./src/**/*",
        "tests"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}